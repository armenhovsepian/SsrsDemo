  <DataSources>
    <DataSource Name="MoviesDataSource">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=.;Initial Catalog=Movies</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>3fbe7c49-0198-46b2-a460-09d72d39b6c0</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="FilmDetails">
      <Query>
        <DataSourceName>MoviesDataSource</DataSourceName>
        <CommandText>SELECT 

tblFilm.FilmID, tblFilm.FilmRunTimeMinutes, tblCertificate.Certificate, tblStudio.StudioName, tblCountry.CountryName, tblGenre.GenreName, tblFilm.FilmOscarWins


FROM tblFilm 
	

INNER JOIN  tblCertificate ON tblFilm.FilmCertificateID = tblCertificate.CertificateID
	
INNER JOIN  tblCountry ON tblFilm.FilmCountryID = tblCountry.CountryID
	
INNER JOIN  tblStudio ON tblFilm.FilmStudioID = tblStudio.StudioID
	
INNER JOIN  tblGenre ON tblFilm.FilmGenreID = tblGenre.GenreId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="FilmID">
          <DataField>FilmID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FilmRunTimeMinutes">
          <DataField>FilmRunTimeMinutes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Certificate">
          <DataField>Certificate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StudioName">
          <DataField>StudioName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CountryName">
          <DataField>CountryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="GenreName">
          <DataField>GenreName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmOscarWins">
          <DataField>FilmOscarWins</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields(Parameters!RowGroupField.Label).Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!FilterValue.Label</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="FilterValueList">
      <Query>
        <DataSourceName>MoviesDataSource</DataSourceName>
        <CommandText>="SELECT " &amp; Parameters!RowGroupField.Label &amp; 
" AS FilterField FROM " &amp; Parameters!RowGroupField.Value &amp; 
" ORDER BY FilterField"</CommandText>
      </Query>
      <Fields>
        <Field Name="FilterField">
          <DataField>FilterField</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
